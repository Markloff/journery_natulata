{
  "extends": "tsconfig/nextjs.json",
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "pages/**/*.env.d.ts"
  ],
  "exclude": [
    "node_modules"
  ],
  "compilerOptions": {
    "strict": false,
    "esModuleInterop": true,
    "strictNullChecks": false,
    "jsx": "preserve",
    "jsxImportSource": "@emotion/react"
  }
}
